{
  "name": "naf-css",
  "version": "3.0.3",
  "description": "A lightweight, progressive, PostCSS, glorified reset thingy.",
  "main": "src/base.css",
  "scripts": {
    "lint": "npx stylelint \"src/**/*.css\"",
    "lint:fix": "npx stylelint \"src/**/*.css\" --fix",
    "build": "npm run lint && node build.js",
    "update:browserlist": "npx browserslist@latest --update-db",
    "dev": "npx http-server "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgrammatiko/naf-css.git"
  },
  "author": "Dimitris Grammatikogiannis <d.grammatiko@gmail.com> (dgrammatiko.online)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "10.4.23",
    "brotli-size": "4.0.0",
    "cssnano": "7.1.2",
    "gzip-size": "7.0.0",
    "kleur": "4.1.5",
    "modern-css-reset": "1.4.0",
    "postcss": "8.5.6",
    "postcss-cli": "11.0.1",
    "postcss-custom-media": "11.0.6",
    "postcss-custom-selectors": "8.0.5",
    "postcss-discard-comments": "7.0.5",
    "postcss-easy-import": "4.0.0",
    "postcss-mixins": "12.1.2",
    "postcss-nested": "7.0.2",
    "postcss-preset-env": "10.6.1",
    "rimraf": "6.1.2",
    "stylelint": "16.26.1",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-order": "6.0.4"
  }
}
