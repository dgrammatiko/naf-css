{
  "name": "naf-css",
  "version": "3.0.1",
  "description": "A lightweight, progressive, PostCSS, glorified reset thingy.",
  "main": "src/base.css",
  "scripts": {
    "lint": "npx stylelint \"src/**/*.css\"",
    "lint:fix": "npx stylelint \"src/**/*.css\" --fix",
    "build": "npm run lint && node build.js",
    "update:browserlist": "npx browserslist@latest --update-db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgrammatiko/naf-css.git"
  },
  "author": "Dimitris Grammatikogiannis <d.grammatiko@gmail.com> (dgrammatiko.online)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "10.4.13",
    "brotli-size": "4.0.0",
    "cssnano": "5.1.14",
    "gzip-size": "7.0.0",
    "kleur": "4.1.5",
    "modern-css-reset": "1.4.0",
    "postcss": "8.4.20",
    "postcss-cli": "10.1.0",
    "postcss-custom-media": "9.0.1",
    "postcss-custom-selectors": "7.0.0",
    "postcss-discard-comments": "5.1.2",
    "postcss-easy-import": "4.0.0",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "6.0.0",
    "postcss-preset-env": "7.8.3",
    "rimraf": "3.0.2",
    "stylelint": "14.16.1",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-order": "6.0.0"
  }
}
