  "scripts": {
    "lint": "npx stylelint \"src/**/*.css\"",
    "lint:fix": "npx stylelint \"src/**/*.css\" --fix",
    "build": "npm run lint && node build.js",
    "update:browserlist": "npx browserslist@latest --update-db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgrammatiko/naf-css.git"
  },
  "author": "Dimitris Grammatikogiannis <d.grammatiko@gmail.com> (dgrammatiko.online)",
  "license": "MIT",
  "devDependencies": {
